{
	"snippets.ultisnips.directories": [ "~/.vim/ultisnips"],
	"snippets.userSnippetsDirectory": "~/.vim/ultisnips",
	"snippets.extends": {
		"typescript": ["javascript"]
	},
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
			"filetypes": ["go"],
			"initializationOptions": {
				"usePlaceholders": true
			}
		}
	}
}
